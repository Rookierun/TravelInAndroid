<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rookie.travelinandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".super_structure.pulgin.PluginApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".super_structure.pulgin.PluginMainActivity"></activity>
        <activity android:name=".super_structure.pulgin.StubActivity" />
        <activity android:name=".super_structure.okhttp.OKHttpActivity" />
        <activity android:name=".super_structure.event_bus.EventBusMainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".super_algorithm.AlgorithmActivity" />
        <activity android:name=".super_flutter.FlutterActivity" />
        <activity android:name=".super_kotlin.KotlinActivity" />
        <activity android:name=".super_ndk.NdkActivity" />
        <activity android:name=".super_performance.PerformanceActivity" />
        <activity android:name=".super_structure.StructureActivity" />
        <activity android:name=".super_ui.UIActivity" />
        <activity
            android:name=".super_structure.ChangeSkinActivity"
            android:configChanges="uiMode" />
        <activity android:name=".super_structure.retrofit.RetrofitActivity" />
        <activity android:name=".super_structure.glide.GlideActivity" />
        <activity
            android:name=".super_structure.plugin_netease.NetEaseProxyActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".super_structure.plugin_netease.NetEaseMainActivity" />
    </application>

</manifest>